@model MemberRegisterViewModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <title>Sign Up - AzerSelling</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <link rel="stylesheet" href="~/assets/register/fonts/material-icon/css/material-design-iconic-font.min.css">

    <link rel="stylesheet" href="~/assets/register/css/style.css">
    <style>


        .button-register {
            padding: 1.3em 3em;
            font-size: 12px;
            text-transform: uppercase;
            letter-spacing: 2.5px;
            font-weight: 500;
            color: #000;
            background-color: #5999e0;
            border: none;
            border-radius: 45px;
            box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);
            transition: all 0.3s ease 0s;
            cursor: pointer;
            outline: none;
        }

            .button-register:hover {
                background-color: #e147f9;
                box-shadow: 0px 5px 10px rgba(140, 127, 134, 0.76);
                color: #fff;
                transform: translateY(-7px);
            }

            .button-register:active {
                transform: translateY(-1px);
            }

        .button-back {
            padding: 1.3em 3em;
            font-size: 12px;
            text-transform: uppercase;
            letter-spacing: 2.5px;
            font-weight: 500;
            color: #000;
            background-color: #e1bb66;
            border: none;
            border-radius: 45px;
            box-shadow: 0px 8px 15px rgba(0, 0, 0, 0.1);
            transition: all 0.3s ease 0s;
            cursor: pointer;
            outline: none;
        }

            .button-back:hover {
                background-color: #8b8b8b;
                box-shadow: 0px 15px 20px rgba(140, 127, 134, 0.76);
                color: #fff;
                transform: translateY(-7px);
            }

            .button-back:active {
                transform: translateY(-1px);
            }
    </style>
    <meta name="robots" content="noindex, follow">
