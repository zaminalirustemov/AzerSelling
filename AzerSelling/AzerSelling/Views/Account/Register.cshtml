<body>
    <div class="main" style="padding-top: 15px;padding-bottom: 15px;">

        <section class="signup">
            <div class="container">
                <div class="signup-content">
                    <div class="signup-form">
                        <h2 class="form-title">Register</h2>
                        <form method="post" class="register-form" id="register-form">
                            <div class="form-group">
                                <label asp-for="Fullname"><i class="zmdi zmdi-account material-icons-name"></i></label>
                                <input asp-for="Fullname" placeholder="Your Fullname" />
                                <span asp-validation-for="Fullname" style="color:red"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Username"><i class="zmdi zmdi-account material-icons-name"></i></label>
                                <input asp-for="Username" placeholder="Your Username" />
                                <span asp-validation-for="Username" style="color:red"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Email"><i class="zmdi zmdi-email"></i></label>
                                <input asp-for="Email" placeholder="Your Email" />
                                <span asp-validation-for="Email" style="color:red"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="PhoneNumber"><i class="zmdi zmdi-phone"></i></label>
                                <input asp-for="PhoneNumber" placeholder="Your Phone" />
                                <span asp-validation-for="PhoneNumber" style="color:red"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Password"><i class="zmdi zmdi-lock"></i></label>
                                <input asp-for="Password" placeholder="Password" />
                                <span asp-validation-for="Password" style="color:red"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="ConfirmPassword"><i class="zmdi zmdi-lock-outline"></i></label>
                                <input asp-for="ConfirmPassword" placeholder="Repeat your password" />
                                <span asp-validation-for="ConfirmPassword" style="color:red"></span>
                            </div>
                            <div style="display:flex">
                                <div class="form-group form-button">
                                    <button type="submit" class="button-register form-submit">Register</button>
                                </div>
                                <div style="padding-top: 25px;padding-left: 10px;margin-top: 11px;">
                                    <a class="button-back" asp-action="index" asp-controller="home" style="text-decoration: none;color:black">
                                        Back
                                    </a>
                                </div>
                            </div>
                        </form>
                    </div>
                    <div class="signup-image" style="align-items: center;display: flex;">
                        <figure>
                            <img src="https://t-cf.bstatic.com/design-assets/assets/v3.51.1/illustrations-traveller/TripsCarRentalManageMyAccount.png" alt="sing up image">
                        </figure>
                    </div>
                </div>
            </div>
        </section>

    </div>

    <script src="~/assets/register/vendor/jquery/jquery.min.js"></script>
    <script src="~/assets/register/js/main.js"></script>

    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13"></script>
